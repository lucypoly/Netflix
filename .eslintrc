{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": [
    "react"
  ],
  "rules": {
    "linebreak-style": "off",
    "no-nested-ternary": "off",
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": "off",
    "jsx-a11y/no-static-element-interactions": "off",
    "react/sort-comp": 0
  },
  "globals": {
    "expect": true,
    "describe": true,
    "test": true,
    "beforeEach": true,
    "window": true,
    "document": true
  }
}
